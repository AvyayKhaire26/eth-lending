    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.5",
    "hardhat": "^2.23.0",
