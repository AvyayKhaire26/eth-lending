{
  "name": "eth-lending",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "node": "hardhat node",
    "deploy": "hardhat run scripts/deploy.ts --network localhost",
    "verify": "hardhat run scripts/verify-deployment.ts --network localhost",
    "start": "concurrently \"npm run node\" \"wait-on tcp:8545 && npm run deploy && npm run verify\"",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "verify:sepolia": "hardhat run scripts/verify-deployment.ts --network sepolia"
  },
